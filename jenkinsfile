


pipeline
{
agent any

		stages
		{
				stage('git clone')
				{ 
					steps
					{
						git 'https://github.com/sadgurukgp/maven-project'

					}	

				}
		}
	

		stage ('compile project')
		{
			steps
			{
				withMaven(jdk: 'gpjdk', maven: 'gpmaven')
					{
						sh 'mvn compile'
					}
			}
		}
}
